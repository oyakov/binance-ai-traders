        "type": "postgres",
        "uid": "postgres-testnet"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres-testnet"
          },
          "rawSql": "SELECT \n  symbol,\n  side,\n  type,\n  status,\n  price,\n  orig_qty as quantity,\n  executed_qty,\n  cummulative_quote_qty as total_value,\n  display_working_time as time\nFROM orders \nORDER BY working_time DESC \nLIMIT 20",
          "refId": "A",
          "format": "table",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "side"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 80
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "BUY": {
                        "color": "green",
                        "text": "BUY"
                      },
                      "SELL": {
                        "color": "red",
                        "text": "SELL"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "status"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "FILLED": {
                        "color": "green",
                        "text": "FILLED"
                      },
                      "NEW": {
                        "color": "blue",
                        "text": "NEW"
                      },
                      "CANCELED": {
                        "color": "yellow",
                        "text": "CANCELED"
                      },
                      "REJECTED": {
                        "color": "red",
                        "text": "REJECTED"
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": []
