{
  "dashboard": {
    "id": null,
    "title": "Kline Data Master Dashboard",
    "tags": ["kline", "master", "overview", "monitoring", "testnet"],
    "style": "dark",
    "timezone": "browser",
    "templating": {
      "list": [
        {
          "name": "symbol",
          "type": "query",
          "query": "label_values(kline_price_open, symbol)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "BTCUSDT",
            "value": "BTCUSDT"
          }
        },
        {
          "name": "interval",
          "type": "query",
          "query": "label_values(kline_price_open, interval)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {
            "selected": true,
            "text": "1m",
            "value": "1m"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"binance-data-collection-testnet\"} + up{job=\"binance-data-storage-testnet\"} + up{job=\"kafka-testnet\"} + up{job=\"postgres-testnet\"} + up{job=\"elasticsearch-testnet\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "5": {"text": "ALL UP", "color": "green"},
                  "4": {"text": "4/5 UP", "color": "yellow"},
                  "3": {"text": "3/5 UP", "color": "orange"},
                  "2": {"text": "2/5 UP", "color": "red"},
                  "1": {"text": "1/5 UP", "color": "red"},
                  "0": {"text": "ALL DOWN", "color": "red"}
                }
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 3},
                {"color": "yellow", "value": 4},
                {"color": "green", "value": 5}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Current Price",
        "type": "stat",
        "targets": [
          {
            "expr": "kline_price_close{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50000},
                {"color": "green", "value": 100000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Data Collection Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(kline_records_total[1m]) * 60",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 60}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Storage Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(kline_records_stored_total / (kline_records_stored_total + kline_storage_errors_total)) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Live Price Chart",
        "type": "timeseries",
        "targets": [
          {
            "expr": "kline_price_open{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "A",
            "legendFormat": "Open"
          },
          {
            "expr": "kline_price_high{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "B",
            "legendFormat": "High"
          },
          {
            "expr": "kline_price_low{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "C",
            "legendFormat": "Low"
          },
          {
            "expr": "kline_price_close{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "D",
            "legendFormat": "Close"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 12, "w": 16, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Volume Chart",
        "type": "timeseries",
        "targets": [
          {
            "expr": "kline_volume{symbol=\"$symbol\", interval=\"$interval\"}",
            "refId": "A",
            "legendFormat": "Volume"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 80,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 12, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 7,
        "title": "Data Collection Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kline_records_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Records/Minute"
          },
          {
            "expr": "rate(kline_messages_received_total[1m]) * 60",
            "refId": "B",
            "legendFormat": "Messages Received/min"
          },
          {
            "expr": "rate(kline_messages_processed_total[1m]) * 60",
            "refId": "C",
            "legendFormat": "Messages Processed/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kline_errors_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Total Errors/min"
          },
          {
            "expr": "rate(kline_websocket_errors_total[1m]) * 60",
            "refId": "B",
            "legendFormat": "WebSocket Errors/min"
          },
          {
            "expr": "rate(kline_storage_errors_total[1m]) * 60",
            "refId": "C",
            "legendFormat": "Storage Errors/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 9,
        "title": "Processing Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(kline_processing_duration_seconds_bucket[5m])) * 1000",
            "refId": "A",
            "legendFormat": "50th percentile (ms)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(kline_processing_duration_seconds_bucket[5m])) * 1000",
            "refId": "B",
            "legendFormat": "95th percentile (ms)"
          },
          {
            "expr": "histogram_quantile(0.99, rate(kline_processing_duration_seconds_bucket[5m])) * 1000",
            "refId": "C",
            "legendFormat": "99th percentile (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 10,
        "title": "Storage Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(kline_records_stored_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "PostgreSQL Records/min"
          },
          {
            "expr": "rate(kline_elasticsearch_indexed_total[1m]) * 60",
            "refId": "B",
            "legendFormat": "Elasticsearch Records/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      },
      {
        "id": 11,
        "title": "Resource Utilization",
        "type": "timeseries",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"binance-data-collection-testnet\", area=\"heap\"} / jvm_memory_max_bytes{job=\"binance-data-collection-testnet\", area=\"heap\"} * 100",
            "refId": "A",
            "legendFormat": "Data Collection Memory %"
          },
          {
            "expr": "jvm_memory_used_bytes{job=\"binance-data-storage-testnet\", area=\"heap\"} / jvm_memory_max_bytes{job=\"binance-data-storage-testnet\", area=\"heap\"} * 100",
            "refId": "B",
            "legendFormat": "Data Storage Memory %"
          },
          {
            "expr": "kline_processing_queue_size",
            "refId": "C",
            "legendFormat": "Processing Queue Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
