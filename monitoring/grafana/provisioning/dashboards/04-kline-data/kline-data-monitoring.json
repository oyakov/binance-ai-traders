    {
      "id": 9,
      "title": "Recent Kline Data Table",
      "type": "table",
      "datasource": {
        "type": "postgres",
        "uid": "postgres-testnet"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres-testnet"
          },
          "rawSql": "SELECT symbol, interval, TO_CHAR(display_time, 'YYYY-MM-DD HH24:MI:SS') as event_time, ROUND(open::numeric, 2) as open, ROUND(high::numeric, 2) as high, ROUND(low::numeric, 2) as low, ROUND(close::numeric, 2) as close, ROUND(volume::numeric, 0) as volume, number_of_trades as trades FROM kline ORDER BY display_time DESC LIMIT 20",
