{
  "id": null,
  "title": "Live Kline Data Monitoring",
  "uid": "kline-data-monitoring",
  "tags": ["kline", "data", "monitoring", "live"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "liveNow": true,
  "refresh": "5s",
  "schemaVersion": 38,
  "style": "dark",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Data Collection",
      "datasource": {
        "type": "postgres",
        "uid": "postgres-testnet"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres-testnet"
          },
          "rawSql": "SELECT CASE WHEN MAX(display_time) > NOW() - INTERVAL '5 minutes' THEN 1 ELSE 0 END as value FROM kline",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "STOPPED", "color": "red"},
                "1": {"text": "COLLECTING", "color": "green"}
              }
            }
          ],
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      },
      "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Storage",
      "targets": [
        {
          "expr": "up{job=\"binance-data-storage-testnet\"}",
