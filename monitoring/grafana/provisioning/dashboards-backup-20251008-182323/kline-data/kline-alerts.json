{
  "id": null,
  "title": "Kline Data Alerts & Health",
  "tags": [
    "kline",
    "alerts",
    "health",
    "monitoring",
    "testnet"
  ],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Critical Alerts",
      "type": "stat",
      "targets": [
        {
          "expr": "kline_websocket_connected == 0",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "NO ALERT",
                  "color": "green"
                },
                "1": {
                  "text": "WEBSOCKET DOWN",
                  "color": "red"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Data Collection Alert",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(kline_records_total[5m]) * 60 < 1",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "NORMAL",
                  "color": "green"
                },
                "1": {
                  "text": "NO DATA",
                  "color": "red"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Storage Alert",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(kline_storage_errors_total[5m]) * 60 > 0",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "NORMAL",
                  "color": "green"
                },
                "1": {
                  "text": "STORAGE ERRORS",
                  "color": "red"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "High Latency Alert",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(kline_processing_duration_seconds_bucket[5m])) > 5",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "NORMAL",
                  "color": "green"
                },
                "1": {
                  "text": "HIGH LATENCY",
                  "color": "red"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "System Health Overview",
      "type": "timeseries",
      "targets": [
        {
          "expr": "up{job=\"binance-data-collection-testnet\"}",
          "refId": "A",
          "legendFormat": "Data Collection"
        },
        {
          "expr": "up{job=\"binance-data-storage-testnet\"}",
          "refId": "B",
          "legendFormat": "Data Storage"
        },
        {
          "expr": "up{job=\"kafka-testnet\"}",
          "refId": "C",
          "legendFormat": "Kafka"
        },
        {
          "expr": "up{job=\"postgres-testnet\"}",
          "refId": "D",
          "legendFormat": "PostgreSQL"
        },
        {
          "expr": "up{job=\"elasticsearch-testnet\"}",
          "refId": "E",
          "legendFormat": "Elasticsearch"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Error Rate Trends",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(kline_websocket_errors_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "WebSocket Errors/min"
