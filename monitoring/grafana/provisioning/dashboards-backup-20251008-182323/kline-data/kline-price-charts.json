{
  "id": null,
  "title": "Live Kline Price Charts",
  "tags": [
    "kline",
    "price",
    "charts",
    "trading",
    "testnet"
  ],
  "style": "dark",
  "timezone": "browser",
  "templating": {
    "list": [
      {
        "name": "symbol",
        "type": "query",
        "query": "label_values(kline_price_open, symbol)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true
      },
      {
        "name": "interval",
        "type": "query",
        "query": "label_values(kline_price_open, interval)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "BTCUSDT Price Chart (1m)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "kline_price_open{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "A",
          "legendFormat": "Open"
        },
        {
          "expr": "kline_price_high{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "B",
          "legendFormat": "High"
        },
        {
          "expr": "kline_price_low{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "C",
          "legendFormat": "Low"
        },
        {
          "expr": "kline_price_close{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "D",
          "legendFormat": "Close"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Volume Chart",
      "type": "timeseries",
      "targets": [
        {
          "expr": "kline_volume{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "A",
          "legendFormat": "Volume"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 80,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      }
    },
    {
      "id": 3,
      "title": "Price Change (24h)",
      "type": "stat",
      "targets": [
        {
          "expr": "((kline_price_close{symbol=\"BTCUSDT\", interval=\"1m\"} - kline_price_open{symbol=\"BTCUSDT\", interval=\"1m\"}) / kline_price_open{symbol=\"BTCUSDT\", interval=\"1m\"}) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": -10
              },
              {
                "color": "yellow",
                "value": 0
              },
              {
                "color": "green",
                "value": 10
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 12
      }
    },
    {
      "id": 4,
      "title": "Current Price",
      "type": "stat",
      "targets": [
        {
          "expr": "kline_price_close{symbol=\"BTCUSDT\", interval=\"1m\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50000
              },
              {
                "color": "green",
                "value": 100000
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 12
      }
    },
    {
      "id": 5,
      "title": "Multi-Symbol Price Comparison",
      "type": "timeseries",
      "targets": [
        {
          "expr": "kline_price_close{symbol=~\"$symbol\", interval=~\"$interval\"}",
          "refId": "A",
          "legendFormat": "{{symbol}} - {{interval}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 20
      }
    },
    {
      "id": 6,
      "title": "Price Volatility (5m)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "((kline_price_high{symbol=\"BTCUSDT\", interval=\"1m\"} - kline_price_low{symbol=\"BTCUSDT\", interval=\"1m\"}) / kline_price_close{symbol=\"BTCUSDT\", interval=\"1m\"}) * 100",
          "refId": "A",
          "legendFormat": "Volatility %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 7,
      "title": "Trading Volume (24h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(kline_volume{symbol=\"BTCUSDT\", interval=\"1m\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "green",
                "value": 10000
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "version": 1,
  "schemaVersion": 36,
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "timepicker": {},
  "uid": "kline-price-charts",
  "weekStart": ""
}