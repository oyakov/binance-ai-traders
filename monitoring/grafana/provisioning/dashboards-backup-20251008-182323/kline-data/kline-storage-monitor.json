{
  "id": null,
  "title": "Kline Storage Monitor",
  "tags": [
    "kline",
    "storage",
    "database",
    "elasticsearch",
    "testnet"
  ],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "PostgreSQL Health",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"postgres-testnet\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Elasticsearch Health",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"elasticsearch-testnet\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Total Records Stored",
      "type": "stat",
      "targets": [
        {
          "expr": "kline_records_stored_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "green",
                "value": 10000
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Storage Success Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "(kline_records_stored_total / (kline_records_stored_total + kline_storage_errors_total)) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 90
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Storage Rate (Records/Minute)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(kline_records_stored_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "PostgreSQL"
        },
        {
          "expr": "rate(kline_elasticsearch_indexed_total[1m]) * 60",
          "refId": "B",
          "legendFormat": "Elasticsearch"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Database Connection Pool",
      "type": "timeseries",
      "targets": [
        {
          "expr": "hikaricp_connections_active{job=\"binance-data-storage-testnet\"}",
          "refId": "A",
          "legendFormat": "Active Connections"
        },
        {
          "expr": "hikaricp_connections_idle{job=\"binance-data-storage-testnet\"}",
          "refId": "B",
          "legendFormat": "Idle Connections"
        },
        {
          "expr": "hikaricp_connections_max{job=\"binance-data-storage-testnet\"}",
          "refId": "C",
          "legendFormat": "Max Connections"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "Elasticsearch Index Health",
      "type": "timeseries",
      "targets": [
        {
          "expr": "elasticsearch_indices_docs_count{index=\"kline\"}",
          "refId": "A",
          "legendFormat": "Document Count"
        },
        {
          "expr": "elasticsearch_indices_store_size_bytes{index=\"kline\"} / 1024 / 1024",
          "refId": "B",
          "legendFormat": "Index Size (MB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Storage Errors by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(kline_postgres_errors_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "PostgreSQL Errors/min"
        },
        {
          "expr": "rate(kline_elasticsearch_errors_total[1m]) * 60",
          "refId": "B",
          "legendFormat": "Elasticsearch Errors/min"
        },
        {
          "expr": "rate(kline_validation_errors_total[1m]) * 60",
          "refId": "C",
          "legendFormat": "Validation Errors/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Storage Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(kline_postgres_write_duration_seconds_bucket[5m])) * 1000",
          "refId": "A",
          "legendFormat": "PostgreSQL 50th percentile (ms)"
        },
        {
          "expr": "histogram_quantile(0.95, rate(kline_postgres_write_duration_seconds_bucket[5m])) * 1000",
          "refId": "B",
          "legendFormat": "PostgreSQL 95th percentile (ms)"
        },
        {
          "expr": "histogram_quantile(0.50, rate(kline_elasticsearch_write_duration_seconds_bucket[5m])) * 1000",
          "refId": "C",
          "legendFormat": "Elasticsearch 50th percentile (ms)"
        },
        {
          "expr": "histogram_quantile(0.95, rate(kline_elasticsearch_write_duration_seconds_bucket[5m])) * 1000",
          "refId": "D",
          "legendFormat": "Elasticsearch 95th percentile (ms)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 10,
      "title": "Data Freshness",
      "type": "timeseries",
      "targets": [
        {
          "expr": "time() - kline_latest_timestamp",
          "refId": "A",
          "legendFormat": "Data Age (seconds)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 60
              },
              {
                "color": "red",
                "value": 300
              }
            ]
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
