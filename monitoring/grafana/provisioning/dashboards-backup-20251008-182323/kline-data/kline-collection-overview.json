    {
      "id": 5,
      "title": "Kline Records Count (Last 5 Minutes)",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(kline_records_total[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "green",
                "value": 5
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Data Collection Rate (Records/Minute)",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(kline_records_total[1m]) * 60",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "green",
                "value": 60
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 7,
      "title": "Kline Data Collection Timeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(kline_records_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "Records/Minute"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(kline_errors_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "Errors/Minute"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Processing Latency (ms)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(kline_processing_duration_seconds_bucket[5m])) * 1000",
          "refId": "A",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(kline_processing_duration_seconds_bucket[5m])) * 1000",
          "refId": "B",
          "legendFormat": "50th percentile"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
